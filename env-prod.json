{
  "name"             : "prod-pos-release-server",
  "out_file": "/data/logs/pos-release-server/prod-pos-release-server-out.log",
  "error_file": "/data/logs/pos-release-server/prod-pos-release-server-error-out.log",
  "script"           : "app.js",
  "max_restarts"     : 50, // defaults to 15
  "exec_mode"        : "fork",
  "autorestart"      : true, // enable/disable automatic restart when an app crashes or exits
  "env": {
    "NODE_ENV": "production"
  }
}